#include <bits/stdc++.h> 
using namespace std; 
  
bool twoPeopleMeet(int d1, int d2, int v1, int v2) 
{  
    if (d1 < d2 && v1 <= v2) 
       return false; 
    if (d1 > d2 && v1 >=v2) 
       return false;   

    if (d1 < d2)  
    {  
        swap(d1, d2);  
        swap(v1, v2);  
    }       
    while (d1 >= d2) 
	{  
        if (d1 == d2)  
            return true; 

        d1 = d1 + v1;  

        d2 = d2 + v2;  
    } 
  
    return false;    
} 
  

int main() 
{ 
    int d1 = 5, v1 = 8, d2 = 4, v2 = 7; 
    if (twoPeopleMeet(d1, d2, v1, v2)) 
        cout<<"True";     
    else 
        cout<<"False"; 
    return 0; 
}
